<!--BMdetail.wxml-->
<view class="wapper">
<view class="list clearfix">
<image class="photo" src="{{dataList.photoPath}}"></image>
<view class="tr" style="border-top: 1px dashed #ccc;">
<label class="label">姓名：</label>
<view class="name">{{dataList.examineeName}}</view>
</view>
<view class="tr">
<label class="label">身份证号：</label>
<view class="name">{{dataList.identity}}</view>
</view>
<view class="tr">
<label class="label">准考证号：</label>
<view class="name">{{dataList.licence}}</view>
</view>
<view class="tr">
<label class="label">考点名称：</label>
<view class="name">{{dataList.examPlaceName}}</view>
</view>
<view class="tr">
<label class="label">考场号：</label>
<view class="name">{{dataList.examRoom}}</view>
</view>
<view class="tr">
<label class="label">座位号：</label>
<view class="name">{{dataList.seatNumber}}</view>
</view>

</view>
</view>
